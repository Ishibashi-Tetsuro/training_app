<h1>日記編集</h1>
<%= form_with model: @diary, method: :patch do |f| %>
  <div class="">
    <%= f.label :content, "今日の日記" %>
    <%= f.text_area :content %>
  </div>
  <div class="">
    <%= f.label :image %>
    <%= f.file_field :image %>
  </div>
  <div class="">
    <%= f.submit "完了", class: "btn btn-primary" %>
  </div>
<% end %>
