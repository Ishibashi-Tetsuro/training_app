<%= form_for(@schedule, url: {controller: 'schedules', action: 'create'}) do |f| %>
  <label class="control-label" for=""><p>エクササイズ</p></label>
  <%= f.text_field :exercise, class: 'form-control' %>

  <label class="control-label" for=""><p>実施予定日</p></label>
  <%= f.date_select :training_date, {:discard_year => true}, class: 'form-control' %>

  <%= f.submit '登録', class: 'btn btn-primary' %>
<% end %>
