<div>
  エクササイズを新規登録
  <%= form_with model: @exercise, local: true, method: :post do |f| %>
    <%= render 'layouts/error_messages', model: f.object %>
    <%= f.label :url, "YouTubeのURL" %>
    <%= f.text_field :url %>
    <%= f.label :part, "Part" %>
    <%= f.select :part, ["二の腕", "お腹", "胸", "足"],
    {include_blank: "---"} %>
    <%= f.label :level, "Level" %>
    <%= f.select :level, ["1","2", "3"],
    {include_blank: "---"} %>
    <div>
      <%= f.submit '登録', class: 'btn btn-primary' %>
    </div>
  <% end %>
</div>
