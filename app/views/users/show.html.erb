<div>
  <h2>ユーザー情報</h2>
  <p>こんにちは<%= @user.id %>さん</p>
  <%= @user.email %>
</div>
<div>
  <h2>投稿一覧</h2>
    <% @diaries.each do |diary| %>
      <p><%= diary.content %></p>
    <% end %>
</div>

<div>
  <h2>エクササイズ</h2>
  <%= link_to "新規エクササイズ登録", new_exercise_path, class: "" %>
  <% @exercises.each do |exercise| %>
    <div>
      <%= exercise.part %>
      <%= exercise.level %>
    </div>
  <% end %>
</div>

<div>
  <h2>スケジュール管理</h2>
  <%= link_to "新規スケジュール設定", new_schedule_path, class: "" %>
  <% @schedules.each do |schedule| %>
    <div>
      <%= schedule.training_date %>
      <%= schedule.part %>
    </div>
  <% end %>
</div>
